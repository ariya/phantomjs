if [ "$CFG_SCRIPT" = "yes" -a "$canBuildQtScript" = "no" ]; then
    echo "Error: QtScript was requested, but it can't be built on this platform."
    exit 1
fi

if [ "$CFG_SCRIPT" = "auto" ]; then
    CFG_SCRIPT="$canBuildQtScript"
