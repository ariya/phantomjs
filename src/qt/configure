		if [ "$QT_MAC_DEFAULT_ARCH" = "x86_64" ]; then
			CFG_MAC_ARCHS=" x86"
		elif [ "$QT_MAC_DEFAULT_ARCH" = "ppc64" ]; then
			CFG_MAC_ARCHS=" ppc"
		else
			CFG_MAC_ARCHS=" $QT_MAC_DEFAULT_ARCH"
		fi
	else
