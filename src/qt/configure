    echo "  12. 12bpp, rgb 4-4-4"
    echo "  15. 15bpp, rgb 5-5-5"
    echo "  16. 16bpp, rgb 5-6-5"
    echo "  18. 18bpp, rgb 6-6-6"
    echo "  24. 24bpp, rgb 8-8-8"
    echo "  32. 32bpp, argb 8-8-8-8 and rgb 8-8-8"
    echo " all. All supported depths"
    echo
